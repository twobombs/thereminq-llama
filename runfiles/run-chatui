#!/bin/bash

# qrack_cl_precompile
/qrack/_build/qrack_cl_precompile

# start webui + API
cd  /text-generation-webui && python3 server.py --listen --share --api --trust-remote-code --gradio-auth thereminq:00000000 &

# start mongod
mongod &

# when fail do not stop container ( debug )
tail -f /dev/null
